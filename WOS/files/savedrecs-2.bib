@article{ ISI:A1992HG30800027,
Author = {HOWELL, MW and THUNELL, RC},
Title = {{ORGANIC-CARBON ACCUMULATION IN BANNOCK BASIN - EVALUATING THE ROLE OF
   PRODUCTIVITY IN THE FORMATION OF EASTERN MEDITERRANEAN SAPROPELS}},
Journal = {{MARINE GEOLOGY}},
Year = {{1992}},
Volume = {{103}},
Number = {{1-3}},
Pages = {{461-471}},
Month = {{JAN}},
Abstract = {{Three sediment cores from Bannock Basin, a hypersaline anoxic basin in
   the eastern Mediterranean, have been studied to evaluate the importance
   of productivity on organic carbon accumulation and sapropel formation in
   this region. Organic carbon accumulation rates were determined for two
   cores from within the anoxic, hypersaline zone and for one core from the
   oxygenated portion of this basin. Organic carbon accumulation rates are
   higher by an order of magnitude in the surficial sediments of the anoxic
   zone due to better preservation. An empirically derived preservation
   factor, which allows calculation of paleoproductivity during the
   deposition of the most recent sapropel (S1), indicates that productivity
   increased by an order of magnitude during the formation of S1. We
   conclude that increased productivity was coupled with a density
   stratification and a circulation reversal in the eastern Mediterranean,
   which together led to sapropel formation.}},
