@inproceedings{ ISI:000088122000002,
Author = {Aiello, LC},
Editor = {{Jablonski, NG and Aiello, LC}},
Title = {{The foundations of human language}},
Booktitle = {{ORIGIN AND DIVERSIFICATION OF LANGUAGE}},
