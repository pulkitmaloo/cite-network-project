@article{ ISI:A1993MJ01400002,
Author = {MILO, RG and QUIATT, D},
Title = {{GLOTTOGENESIS AND ANATOMICALLY MODERN HOMO-SAPIENS - THE EVIDENCE FOR
   AND IMPLICATIONS OF A LATE ORIGIN OF VOCAL LANGUAGE}},
Journal = {{CURRENT ANTHROPOLOGY}},
Year = {{1993}},
Volume = {{34}},
Number = {{5}},
Pages = {{569-598}},
Month = {{DEC}},
Abstract = {{Recent debate concerning the origins of human language has focused on
   such questions as whether nonmodern hominids possessed any symbolic
   capacity and whether they were morphologically capable of speaking. We
   argue from an evolutionary perspective that language is one
   manifestation of a distinctively human cognitive-behavioral complex
   which appeared with genus Homo and which has been under strong selective
   pressure, albeit in different ways at different times, throughout the
   course of human evolution. Informed by the view that modem humans arose
   as the result of an evolutionary event in a single population of
   nonmodern hominids, we suggest that this event created the neurological
   and morphological conditions under which the cognitive and linguistic
   potentialities of prior hominids could be fully realized. We find
   support for a late origin of rapidly spoken phonemic speech in the
   evidence from paleontology and archaeology bearing on the behavioral and
   cultural complexity of nonmodern and modem humans. We conclude,
   following Hewes, that vocal-auditory human language as we know it
   today-phonemicized, syntactical, and infinitely open and
   productive-arose under selection for increasing efficiency and
   flexibility of information storage and retrieval. We note the
   implications for group selection at this particular juncture in human
   evolution.}},
