@article{ ISI:A1991GA44800004,
Author = {JOHNSON, TC and HALFMAN, JD and SHOWERS, WJ},
Title = {{PALEOCLIMATE OF THE PAST 4000 YEARS AT LAKE TURKANA, KENYA, BASED ON THE
   ISOTOPIC COMPOSITION OF AUTHIGENIC CALCITE}},
Journal = {{PALAEOGEOGRAPHY PALAEOCLIMATOLOGY PALAEOECOLOGY}},
Year = {{1991}},
Volume = {{85}},
Number = {{3-4}},
Pages = {{189-198}},
Month = {{JUN}},
Abstract = {{Authigenic calcite was analyzed for delta-C-13 and delta-O-18 in a 12 m
   long core from Lake Turkana that spans the last 4200 yr. The delta-O-18
   record is interpreted to reflect lake level history, and indicates high
   stands prior to 4000 yr B.P., from 1800 to 2000 yr B.P., and within the
   last century. This coincides better with the history of lake level
   change reported for other lakes in Africa than did the previous lake
   level curve for Lake Turkana based on C-14 dates of exposed shoreline
   deposits. The delta-C-13 curve roughly parallels the delta-O-18 curve,
   and may reflect the influence of the Omo River plume. Both of the
   isotope records are strongly cyclic, with periodicities common to both
   at 80, 120, 150 and 225 yr. Other, somewhat longer periodicities are
   nearly equal in the two records, and there is close agreement with
   previously published cycles based on carbonate and lamination thickness
   analyses in the same Turkana core.}},
