@article{ ISI:A1996VH44700004,
Author = {Kornfeld, M},
Title = {{The big-game focus - Reinterpreting the archaeological record of
   Cantabrian Upper Paleolithic economy}},
Journal = {{CURRENT ANTHROPOLOGY}},
Year = {{1996}},
Volume = {{37}},
Number = {{4}},
Pages = {{629-657}},
Month = {{AUG-OCT}},
Abstract = {{Big game and big-game hunting play a major role in hominid evolutionary
   models, cultural evolutionary models, and ethnographic descriptions. In
   western Europe's Late and Terminal Pleistocene sites, the remains of big
   game, including red deer, are common, and these remains figure
   prominently in economic re constructions. However, assumptions about the
   dietary role of big-game animals guide critical aspects of these
   reconstructions, eliminating from consideration alternative economic
   strategies. Since archaeological data are usually ambiguous, inferences
   and models about the past can be substantially altered by applying
   different assumptions and methodologies to the same data. The case of
   Pleistocene Cantabria is used to show how an economy focused on
   resources other than big-game may be modeled.}},
