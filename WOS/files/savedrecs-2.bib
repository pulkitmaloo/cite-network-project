@article{ ISI:A1995RU88100006,
Author = {BERTRAN, P and TEXIER, JP},
Title = {{FABRIC ANALYSIS - APPLICATION TO PALEOLITHIC SITES}},
Journal = {{JOURNAL OF ARCHAEOLOGICAL SCIENCE}},
Year = {{1995}},
Volume = {{22}},
Number = {{4}},
Pages = {{521-535}},
Month = {{JUL}},
Abstract = {{This paper reviews data on the strike and dip (''fabric'') of clasts in
   slope deposits and stresses the usefulness of such investigations in
   regard to the study of prehistoric living floors, particularly to test
   the occurrence of disturbance processes. Nine Paleolithic sites have
   been studied for this purpose. Five of them, located in rockshelters or
   cave entrances, show a preferred orientation of the artefacts along the
   slope. This pattern appears to be the result of periglacial mass
   movements. So far such processes have been largely underestimated and
   may explain the absence of clear anthropogenic structures at these
   sites. (C) 1995 Academic Press Limited}},
