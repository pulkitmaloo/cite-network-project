@article{ ISI:A1992JJ54900008,
Author = {SCHLUCHTER, C},
Title = {{TERRESTRIAL QUATERNARY STRATIGRAPHY}},
Journal = {{QUATERNARY SCIENCE REVIEWS}},
Year = {{1992}},
Volume = {{11}},
Number = {{5}},
Pages = {{603-607}},
Abstract = {{The status and use of terrestrial records for the reconstruction of
   Quaternary Stratigraphy is considered. A summary of the historical
   background along three major lines of evolution of knowledge (classical,
   isotope-stratigraphy, advanced terrestrial records) is followed by a
   discussion of limiting factors to Quaternary terrestrial stratigraphy as
   a result of exo- and endogenetic processes. A verification system for
   global paleoclimatic models can be derived from detailed geological
   facies analysis of the sedimentary record of terrestrial reference
   sections. With the example of the Quaternary records from Antarctica a
   model based on terrestrial records is presented as prerequisite for
   global reconstructions and as demonstration of sea/land interactions.}},
