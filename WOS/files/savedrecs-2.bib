@article{ ISI:A1991JX30100005,
Author = {GIUSBERTI, G and PERETTO, C},
Title = {{EVIDENCE OF INTENTIONAL ANIMAL MARROW-BONE BREAKAGE IN THE PALEOLITHIC
   SITE OF LA-PINETA-DI-ISERNIA, MOLISE, ITALY}},
Journal = {{ANTHROPOLOGIE}},
Year = {{1991}},
Volume = {{95}},
Number = {{4}},
Pages = {{765-778}},
Abstract = {{Several fragments of Bison (Bison schoetensacki FREUD.) long bones found
   in one of the anthropic ancient surfaces of the deposit ``La Pineta{''}
   Isernia (Italy (736 000 +/- 40 000 years, K/Ar), show unequivocal
   evidence of systematic marrow-bone breakage made by palaeolithic man.
   Large semi-circular notches, produced by strong percussions and probably
   made with hammer stones, were observed.
   The bone remains found on the paleosurface (t. 3a, sector I) are being
   studied. The present state of research points out what follows: some
   long bones of Bison front leg were broken by paleolithic man; several
   bones of the same type show evident percussion marks in the same
   anatomical point; the size of the large semicircular impact notches is 4
   - 9 cm; such impact notches are frequently observed in the following
   bones (fig. 9): Humerus: in the lateral face of distal shaft (gutter
   area); Radius: in the anterior face of shaft immediately under a
   proximal articular end; Metacarpus: in the posterior face of the half
   proximal shaft.}},
