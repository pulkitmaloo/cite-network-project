@article{ ISI:A1995RB29600010,
Author = {STINER, MC and KUHN, SL and WEINER, S and BARYOSEF, O},
Title = {{DIFFERENTIAL BURNING, RECRYSTALLIZATION, AND FRAGMENTATION OF
   ARCHAEOLOGICAL BONE}},
