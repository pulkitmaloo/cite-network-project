@article{ ISI:A1994NM45100014,
Author = {MAREAN, CW and MUDIDA, N and REED, KE},
Title = {{HOLOCENE PALEOENVIRONMENTAL CHANGE IN THE KENYAN CENTRAL RIFT AS
   INDICATED BY MICROMAMMALS FROM ENKAPUNE-YA-MUTO ROCKSHELTER}},
Journal = {{QUATERNARY RESEARCH}},
Year = {{1994}},
Volume = {{41}},
Number = {{3}},
Pages = {{376-389}},
Month = {{MAY}},
Abstract = {{An assemblage of micromammals, recovered from the Holocene levels of a
   rockshelter at 2400 m in the montane forest of the Mau Escarpment, were
   examined with the goal of testing and contributing to prior
   reconstructions of paleoenvironments in the Central Rift Valley of
   Kenya. Species representation in the assemblage is consistent with a
   drying of the Rift Valley lakes in the middle Holocene and suggests a
   decrease in forest accompanied by expanding grasslands near the site.
   Changes in the abundance of grassland species suggests an increase in
   the frequency of fires, probably the result of pastoral burning. The
   body size of the root rat (Tachyoryctes splendens) decreases from the
   early Holocene to the middle Holocene, and this may indicate increasing
   aridity or increasing temperature. We compare measures of species
   diversity (number of taxa, species richness, and the Shannon diversity
   index) for both micromammals and macromammals since species diversity
   may change with paleoenvironmental change. The macromammals show no
   changes in species diversity that are assignable to paleoenvironmental
   change, while the micromammals show a trend toward decreasing diversity
   from the early to middle Holocene, and then show an increase in
   diversity during the peak of the middle Holocene dry phase, though
   sample size effects may be confounding the patterning.    (C) 1994
   University of Washington.}},
