@article{ ISI:A1991HF54700002,
Author = {PARTRIDGE, TC and TOBIAS, PV and HUGHES, AR},
Title = {{PALEOECOLOGY AND SOME AFFINITIES OF THE SOUTHERN AFRICAN
   AUSTRALOPITHECINES - NEW DATA FROM STERKFONTEIN AND TAUNG}},
Journal = {{ANTHROPOLOGIE}},
Year = {{1991}},
Volume = {{95}},
Number = {{2-3}},
Pages = {{363-378}},
Abstract = {{Specimens of Australopithecus africanus from Sterkfontein all derive
   from Member 4, which probably dates to between 2.8 and 2.4 Myr.  Marked
   aridification towards the end of this period seems to have coincided
   with the disappearance of this species and with the appearance, in
   Member 5, of Homo habilis, stone tool making, and a different faunal
   assemblage.  A major speciation event, initiated by climatic forcing, is
   indicated.
   New evidence from Taung adds weight to the claimed young age of 1.0 -
   1.2 Myr for the juvenile specimen of A. africanus from this site.  If
   this young dating is correct, the late survival of this lineage at
   Taung, in association with an apparently older faunal assemblage, is
   best explained by the existence of a local refugium.  Palaeoclimatic and
   palaeohydrological data are cited in support of this explanation.}},
