@article{ ISI:A1993LQ53200003,
Author = {MOCHANOV, YA},
Title = {{THE MOST ANCIENT PALEOLITHIC OF THE DIRING AND THE PROBLEM OF A
   NONTROPICAL ORIGIN FOR HUMANITY}},
Journal = {{ARCTIC ANTHROPOLOGY}},
Year = {{1993}},
Volume = {{30}},
Number = {{1}},
Pages = {{22-53}},
Abstract = {{During a test excavation at Diring-Yuriakh Creek, c. 145 km up the Lena
   River from Yakutsk, human remains dating to the Neolithic Period were
   found. The excavation was turned over to Yu. A. Mochanov and expanded,
   at which time a Paleolithic stone tool assemblage was discovered.
   Analysis of the artifacts and of their paleoenvironmental context
   suggest a date for the tools of 1.8 to 3.2 million years. The tools more
   closely resemble those from Olduvai Gorge than from any other Early
   Pleistocene site, leading the author to ponder the possibility of a
   nontropical origin for humanity.}},
