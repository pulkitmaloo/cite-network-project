@article{ ISI:000257995900016,
Author = {van der Merwe, Nikolaas J. and Masao, Fidelis T. and Bamford, Marion K.},
Title = {{Isotopic evidence for contrasting diets of early hominins Homo habilis
   and Australopithecus boisei of Tanzania}},
Journal = {{SOUTH AFRICAN JOURNAL OF SCIENCE}},
Year = {{2008}},
Volume = {{104}},
Number = {{3-4}},
Pages = {{153-155}},
Month = {{MAR-APR}},
Abstract = {{Isotopic dietary studies of early hominins have hitherto been confined
   to specimens from South Africa. We are now able to report isotopic
   analyses of two species of early hominins from Tanzania: Homo habilis
   and Australopithecus boisei. The results show that these two species had
   very different diets. The isotopic analyses of three South African
   species of early hominins, in contrast, show considerable variation in
   individual diets, but no marked differences between species.}},
