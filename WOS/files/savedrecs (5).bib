@article{ ISI:000266187500022,
Author = {Markovic, Slobodan B. and Hambach, Ulrich and Catto, Norm and Jovanovic,
   Mladjen and Buggle, Bjoern and Machalett, Bjoern and Zoeller, Ludwig and
   Glaser, Bruno and Frechen, Manfred},
Title = {{Middle and Late Pleistocene loess sequences at Batajnica, Vojvodina,
   Serbia}},
