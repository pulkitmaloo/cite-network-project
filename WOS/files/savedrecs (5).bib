@article{ ISI:000242130800008,
Author = {Norton, Christopher J. and Bae, Kidong and Harris, John W. K. and Lee,
   Hanyong},
Title = {{Middle Pleistocene handaxes from the Korean Peninsula}},
Journal = {{JOURNAL OF HUMAN EVOLUTION}},
