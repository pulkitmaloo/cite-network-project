@article{ ISI:000175910300044,
Author = {Mercader, J and Panger, M and Boesch, C},
Title = {{Excavation of a chimpanzee stone tool site in the African rainforest}},
Journal = {{SCIENCE}},
Year = {{2002}},
Volume = {{296}},
Number = {{5572}},
Pages = {{1452-1455}},
Month = {{MAY 24}},
Abstract = {{Chimpanzees from the Tai forest of Cote d'lvoire produce unintentional
   flaked stone assemblages at nut-cracking sites, leaving behind a record
   of tool use and plant consumption that is recoverable with
   archaeological methods. About 40 kilograms of nutshell and 4 kilograms
   of stone were excavated at the Panda 100 site. The data unearthed show
   that chimpanzees transported stones from outcrops and soils to focal
   points, where they used them as hammers to process foodstuff. The
   repeated use of activity areas led to refuse accumulation and site
   formation. The implications of these data for the interpretation of the
   earliest hominin archaeological record are explored.}},
